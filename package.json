  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts --max_old_space_size=8192 build",
    "serve": "serve -s build -p 3000",
    "cy:open": "cypress open",
    "cy:open:chrome": "cypress open --browser chrome",
    "cy:run": "cypress run --browser chromium",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:record": "cypress run --browser chromium --record --key 39cedb95-c4ef-49e1-9070-3b7cb2ec49fb",
    "format": "prettier --write --loglevel warn",
    "lint:all:format": "yarn format \"**/*.{jsx,js,ts,tsx,json,css,scss,html,md}\"",
    "lint": "yarn lint:js:all && yarn lint:md:all",
    "lint:js": "eslint --max-warnings=0",
    "lint:js:all": "yarn lint:js --ext js,jsx,ts,tsx src tests",
    "lint:js:fix": "yarn lint:js:all --fix",
    "lint:md": "markdownlint --ignore public --ignore .github --ignore CHANGELOG.md ",
    "lint:md:all": "markdownlint '**/*.md' --ignore public --ignore node_modules  --ignore CHANGELOG.md --ignore build ",
    "lint:scss": "stylelint",
    "lint:scss:all": "stylelint 'src/**/*.scss'",
    "download:config": "mkdir -p public/config/ && scripts/local-version.sh && curl -o public/config/config.json",
    "start:dev": "react-scripts --max_old_space_size=4096 start",
    "start": "yarn download:config https://sample-server.sample.com/config/config.json && yarn start:dev",
    "test": "react-scripts test",
    "test:unit:file": "CI=true yarn test --findRelatedTests ",
    "test:coverage": "CI=true yarn test --coverage ",
    "test:e2e": "BROWSER=none start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "BROWSER=none start-server-and-test start http://localhost:3000 cy:open:chrome",
    "test:e2e:ci": "start-server-and-test test:e2e:serve http://localhost:3000 cy:run",
    "test:e2e:record": "start-server-and-test start http://localhost:3000 cy:run:record",
    "test:e2e:serve": "serve --no-clipboard --single --listen 3000 build",
    "version:major": "yarn version --major --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:minor": "yarn version --minor --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:patch": "yarn version --patch --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:beta": "yarn version --prerelease --preid beta --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:hotfix": "yarn version --prerelease --preid hotfix --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:rc": "yarn version --prerelease --preid rc --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:tag": "git commit -a -m v$npm_package_version --no-verify && git tag -a v$npm_package_version -m v$npm_package_version && git push --follow-tags && echo \"Pushed version v$npm_package_version!\"",
    "postinstall": "patch-package"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not chrome < 51",
      "not safari < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
